<settings>
  <font>Tahoma</font>
  <type>{lineType}</type>
  <decimals_separator>.</decimals_separator>
 
  <background>  
    <alpha>100</alpha>
    <border_alpha>15</border_alpha>
  </background>
  
  <plot_area>
    <margins>
      <left>{lineTypeLeft}</left>
      <right>{lineTypeRight}</right>
      <top>80</top>
      <bottom>{lineTypeBottom}</bottom>
   </margins>
  </plot_area>
  
  <grid>
    <category>
      <alpha>5</alpha>
    </category>
    <value>
      <alpha>5</alpha>
    </value>
  </grid>
  
  <axes>
    <category>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
    </value>
  </axes>
  
  <values>
    <category>
      <color>999999</color>
    </category>
    <value>
      <min>0</min>
    </value>
  </values>
  
  <legend>
    <enabled>0</enabled>
 </legend>
 
  <column>
    <width>85</width>
    <spacing>0</spacing>
    <alpha>80</alpha>
    <corner_radius_top>8</corner_radius_top>
    <balloon_text></balloon_text>
    <grow_time>3</grow_time>
    <sequenced_grow>1</sequenced_grow>
    <data_labels>
      <![CDATA[{value}%]]>
    </data_labels>
  </column>
  
  <line>
    <balloon_text></balloon_text>
  </line>
  
  <graphs>
    <graph gid="1">
      <title>Visits</title>
      <color>B92F2F</color>
      <balloon_text>{series}: {value}%</balloon_text>
    </graph>
    <graph gid="2">
      <title>Smoothed</title>
      <type>line</type>
    </graph>
  </graphs>
  
  <labels>
    <label lid="0">
      <text><![CDATA[<b>{questionName}<br>(N={totalValue})</b>]]></text>
      <y>30</y>
      <align>center</align>
      <text_size>14</text_size>
   </label>
  </labels>
  
  <guides>
    <guide gid="1"/>
  </guides>
  
  <export_as_image>
    <file>../Chart/ChartExport.php</file> 
    <target>_top</target>
    <x></x>
    <y></y>
    <color></color>
    <alpha>100</alpha>
    <text_color></text_color>
    <text_size></text_size>
  </export_as_image>
  
  <strings>
    <no_data></no_data>
    <export_as_image>以图片格式导出</export_as_image>
    <collecting_data>收集数据过程中...</collecting_data>
    <ss></ss>
    <mm></mm>
    <hh></hh>
    <DD></DD>  
  </strings>  

  <context_menu>
    <default_items>
	  <zoom>false</zoom>
	  <print>false</print>
	</default_items>

  </context_menu>  

</settings>